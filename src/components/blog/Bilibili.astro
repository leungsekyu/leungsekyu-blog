---
interface Props {
  url: string
}

const { url } = Astro.props
---

<iframe
  src={`${url}&autoplay=0`}
  title="Bilibili video player"
  scrolling="no"
  border="0"
  frameborder="no"
  framespacing="0"
  allowfullscreen="true"
  class="aspect-[4/3] w-full max-w-[653.109px] md:aspect-[16/10]"></iframe>
